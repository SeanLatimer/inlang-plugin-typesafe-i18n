{
  "name": "inlang-plugin-typesafe-i18n",
  "type": "module",
  "module": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:watch\" \"npm run dev:test\"",
    "dev:test": "vitest",
    "dev:watch": "esbuild ./src/index.ts --bundle --platform=browser --format=esm --minify --outfile=./dist/index.js --watch",
    "build": "esbuild ./src/index.ts --bundle --platform=browser --format=esm --minify --outfile=./dist/index.js",
    "test": "npm run tsc && npm run vitest",
    "vitest": "vitest run",
    "tsc": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@inlang/core": "^0.3.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.4",
    "husky": "^8.0.3",
    "memfs": "^3.4.13",
    "typesafe-i18n": "^5.20.0",
    "typescript": "^4.9.4",
    "vitest": "^0.27.3"
  },
  "volta": {
    "node": "18.13.0"
  }
}
